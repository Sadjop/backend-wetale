{% extends 'base.html.twig' %}

{% block title %}SubscriptionType{% endblock %}

{% block body %}
    <h1>SubscriptionType</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ subscription_type.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ subscription_type.name }}</td>
            </tr>
            <tr>
                <th>Duration</th>
                <td>{{ subscription_type.duration }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ subscription_type.price }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ subscription_type.createdAt ? subscription_type.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ subscription_type.updatedAt ? subscription_type.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_subscription_type_index') }}">back to list</a>

    <a href="{{ path('app_subscription_type_edit', {'id': subscription_type.id}) }}">edit</a>

    {{ include('subscription_type/_delete_form.html.twig') }}
{% endblock %}
